pipeline {
    agent any

    stages {
        stage('Checkout') {
      steps {
        checkout scm
        script {          
              git branch: 'Main', credentialsId: 'Git', url: 'https://github.com/YogeshBaldha/ColudboostTest-Jenkins.git'
            }
          }
        }
      }
    }
        stage('Build') {
            steps {
                echo 'Building..'
            }
        }               
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
